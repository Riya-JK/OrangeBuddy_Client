<UserControl x:Class="OrangeBuddy_Client.UserControls.MultiDateTimePicker"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:OrangeBuddy_Client.UserControls" Name="multiDateTimePicker">
    <StackPanel Orientation="Horizontal" >
        <DatePicker Name="datePicker"/>
        <StackPanel Orientation="Horizontal" Margin="2" >
            <ComboBox Name="hourComboBox"/>
            <ComboBox Name="minuteComboBox"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal" Margin="2" >
            <ComboBox Name="endhourComboBox"/>
            <ComboBox Name="endminuteComboBox"/>
        </StackPanel>
    </StackPanel>
    
</UserControl>
